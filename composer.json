{
	"name": "idr0id/papper",
	"type": "library",
	"description": "Papper is PHP convention-based object to object mapper",
	"keywords": ["automapper", "mapper", "DTO"],
	"license": "MIT",
	"authors": [
		{
			"name": "Vladimir Komissarov",
			"homepage": "http://dr0id.ru"
		}
	],
	"require" : {
		"php": ">=5.3.3",
		"symfony/property-access": "~2.4",
		"andrewsville/php-token-reflection": "~1.3"
	},
	"autoload": {
		"psr-0": {
			"Papper": "src"
		}
	},
	"autoload-dev": {
		"psr-0": {
			"Papper\\Tests": "tests"
		}
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"mockery/mockery": "~0.9"
	}
}
